@model IEnumerable<MvcSeriesPersonajesPCM.Models.Personajes>

<h1>Personajes</h1>


<table class="table table-dark">
    <thead>
        <tr>
            <th>
                <h3>Nombre</h3>
            </th>
            <th>
                <h3>Imagen</h3>
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>

            <td>
                <img src="@item.Imagen" width="300" height="300">
            </td>

        </tr>
}
    </tbody>
</table>
<a asp-controller="Series" asp-action="Index"
   class="btn btn-outline-danger"> Volver Atrás</a>
